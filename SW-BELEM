!
! =============================================
! SWITCH BELÉM - SW-BELEM
! Projeto Redes Belém e Castanhal
! =============================================
!

enable
conf t
hostname SW-BELEM

vlan 10
 name PREFEITURA
vlan 20
 name HOSPITAL

interface fa0/1
 switchport mode trunk
 switchport trunk allowed vlan 10,20

interface fa0/2
 switchport mode access
 switchport access vlan 10

interface fa0/3
 switchport mode access
 switchport access vlan 20

end
wr
